{
  "extends": "../tsconfig.json",
  "include": [
    "./**/*.ts*"
  ],
  "exclude": [],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/common/*": [
        "../src/common/*"
      ],
      "@/components/*": [
        "../src/common/components/*"
      ],
      "@/utils/*": [
        "../src/common/utils/*"
      ],
      "@/modules/*": [
        "../src/modules/*"
      ]
    },
    "types": [
      "cypress"
    ],
    "sourceMap": false,
    "isolatedModules": true
  }
}
